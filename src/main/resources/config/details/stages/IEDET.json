{
  "type": "IEDET",
  "groups": {
    "IEDET_REGISTRATION": [
      {
        "component": "date",
        "name": "DateReceived",
        "label": "When was the correspondence received?"
      },
      {
        "component": "entity-list",
        "props": {
          "action": "CORRESPONDENT",
          "entity": "correspondent",
          "choices": "CASE_CORRESPONDENTS",
          "addUrlPath": "addNoMp",
          "hasAddLink": true,
          "hasEditLink": true,
          "hasRemoveLink": true,
          "hideRemovePrimary": true
        },
        "name": "Correspondents",
        "label": "Who is the primary correspondent?"
      },
      {
        "component": "date",
        "name": "ComplainantDOB",
        "label": "Date of birth"
      },
      {
        "component": "radio",
        "props": {
          "choices": [
            {
              "label": "Male",
              "value": "Male"
            },
            {
              "label": "Female",
              "value": "Female"
            }
          ]
        },
        "name": "ComplainantGender",
        "label": "Gender"
      },
      {
        "component": "dropdown",
        "props": {
          "choices": "COUNTRIES_CURRENT"
        },
        "name": "ComplainantNationality",
        "label": "Nationality"
      },
      {
        "component": "text",
        "name": "ComplainantCompanyName",
        "label": "Company name"
      },
      {
        "component": "text",
        "name": "ComplainantHORef",
        "label": "Home Office reference"
      },
      {
        "component": "text",
        "name": "ComplainantPortRef",
        "label": "Port reference"
      }
    ],
    "IEDET_TRIAGE": [
      {
        "component": "text-area",
        "name": "CaseSummary",
        "label": "Case summary"
      },
      {
        "component": "radio",
        "props": {
          "choices": [
            {
              "label": "Third party supplier",
              "value": "ThirdParty"
            },
            {
              "label": "IE Detention compliance team",
              "value": "IedetCompliance"
            },
            {
              "label": "DEPMU",
              "value": "DEPMU"
            },
            {
              "label": "Send to CCH",
              "value": "CCH"
            }
          ],
          "className": "govuk-radios--small"
        },
        "name": "TriageAssign",
        "label": "Who will investigate the complaint?"
      },
      {
        "component": "dropdown",
        "props": {
          "choices": "IEDET_BUSINESS_AREAS",
          "visibilityConditions": [
            {
              "conditionPropertyName": "TriageAssign",
              "conditionPropertyValue": "ThirdParty"
            },
            {
              "conditionPropertyName": "TriageAssign",
              "conditionPropertyValue": "IedetCompliance"
            },
            {
              "conditionPropertyName": "TriageAssign",
              "conditionPropertyValue": "DEPMU"
            }
          ]
        },
        "name": "BusinessArea",
        "label": "Business area"
      },
      {
        "component": "text-area",
        "props": {
          "visibilityConditions": [
            {
              "conditionPropertyName": "TriageAssign",
              "conditionPropertyValue": "CCH"
            }
          ]
        },
        "name": "TransferCaseNote",
        "label": "Reason for transfer"
      },
      {
        "component": "dropdown",
        "props": {
          "choices": "S_IEDET_COMP_ORIGIN"
        },
        "name": "CompOrigin",
        "label": "Complaint origin"
      },
      {
        "component": "text",
        "props": {
          "limit": 100,
          "visibilityConditions": [
            {
              "conditionPropertyName": "CompOrigin",
              "conditionPropertyValue": "Other"
            }
          ]
        },
        "name": "CompOriginOther",
        "label": "Other Complaint Origin"
      },
      {
        "component": "text",
        "name": "3rdPartyRef",
        "label": "Third party reference"
      },
      {
        "component": "paragraph",
        "props": {
          "children": "Select all that apply."
        },
        "name": "categoryInset"
      },
      {
        "props": {
          "choices": [
            {
              "label": "Admin / process error",
              "value": "AdminErr",
              "name": "CatAdminErr"
            },
            {
              "label": "Availability of service",
              "value": "Avail",
              "name": "CatAvail"
            },
            {
              "label": "Complaint handling",
              "value": "Handle",
              "name": "CatHandle"
            },
            {
              "label": "Delay",
              "value": "Delay",
              "name": "CatDelay"
            },
            {
              "label": "Physical environment",
              "value": "PhysEnv",
              "name": "CatPhysEnv"
            },
            {
              "label": "Poor communication",
              "value": "PoorComm",
              "name": "CatPoorComm"
            },
            {
              "label": "Property or documents - lost",
              "value": "Lost",
              "name": "CatLost"
            },
            {
              "label": "Property or documents - stolen",
              "value": "Stolen",
              "name": "CatStolen"
            },
            {
              "label": "Property or documents - withheld",
              "value": "Withheld",
              "name": "CatWithheld"
            },
            {
              "label": "Provision for minors",
              "value": "ProvMinor",
              "name": "CatProvMinor"
            },
            {
              "label": "Wrong information",
              "value": "WrongInfo",
              "name": "CatWrongInfo"
            }
          ],
          "className": "govuk-checkboxes--small",
          "showLabel": true,
          "saveSeparately": true
        },
        "component": "checkbox-group",
        "name": "CompCategoriesService",
        "label": "Service"
      },
      {
        "props": {
          "choices": [
            {
              "label": "Detainee on detainee",
              "value": "DetOnDet",
              "name": "CatDetOnDet"
            },
            {
              "label": "Other unprofessionalism",
              "value": "OtherUnprof",
              "name": "CatOtherUnprof"
            },
            {
              "label": "Rudeness",
              "value": "Rude",
              "name": "CatRude"
            },
            {
              "label": "Unfair treatment",
              "value": "Unfair",
              "name": "CatUnfair"
            }
          ],
          "className": "govuk-checkboxes--small",
          "showLabel": true,
          "saveSeparately": true
        },
        "component": "checkbox-group",
        "name": "CompCategoriesSeriousAndMinor",
        "label": "Serious and minor misconduct"
      },
      {
        "props": {
          "choices": [
            {
              "label": "Assault",
              "value": "Assault",
              "name": "CatAssault"
            },
            {
              "label": "Fraud / corruption",
              "value": "Fraud",
              "name": "CatFraud"
            },
            {
              "label": "Racism / discrimination",
              "value": "Racism",
              "name": "CatRacism"
            },
            {
              "label": "Sexual assault",
              "value": "SexAssault",
              "name": "CatSexAssault"
            },
            {
              "label": "Theft",
              "value": "Theft",
              "name": "CatTheft"
            }
          ],
          "className": "govuk-checkboxes--small",
          "showLabel": true,
          "saveSeparately": true
        },
        "component": "checkbox-group",
        "name": "CompCategoriesSerious",
        "label": "Serious misconduct"
      },
      {
        "component": "radio",
        "props": {
          "choices": [
            {
              "label": "Service",
              "value": "Service"
            },
            {
              "label": "Minor misconduct",
              "value": "MinorMisconduct"
            },
            {
              "label": "Serious misconduct",
              "value": "SeriousMisconduct"
            }
          ],
          "className": "govuk-radios--small",
          "showLabel": false
        },
        "name": "CompType",
        "label": "Complaint type"
      }
    ],
    "IEDET_DRAFT": [
      {
        "component": "entity-list",
        "props": {
          "entity": "document",
          "choices": "CASE_DOCUMENT_LIST_ALL",
          "hasAddLink": true,
          "hasRemoveLink": true
        },
        "name": "ResponseDocuments",
        "label": "Response document"
      },
      {
        "component": "entity-manager",
        "props": {
          "entity": "template",
          "choices": "CASE_TEMPLATES",
          "hasTemplateLink": true
        },
        "name": "Complaint_Response",
        "label": "Available Template"
      }
    ],
    "IEDET_SEND": [
      {
        "component": "entity-list",
        "props": {
          "entity": "document",
          "choices": "CASE_DOCUMENT_LIST_IEDET_FINAL_RESPONSE",
          "hasAddLink": true,
          "hasRemoveLink": true
        },
        "name": "FinalResponseDocument",
        "label": "Upload the Final Response"
      },
      {
        "choices": [
          {
            "label": "Substantiated",
            "value": "Substantiated"
          },
          {
            "label": "Unsubstantiated",
            "value": "Unsubstantiated"
          },
          {
            "label": "Partially Substantiated",
            "value": "PartiallySubstantiated"
          },
          {
            "label": "Withdrawn",
            "value": "Withdrawn"
          },
          {
            "label": "Other",
            "value": "Other"
          }
        ],
        "name": "CaseOutcome",
        "label": "Case outcome"
      },
      {
        "component": "text-area",
        "props": {
          "visibilityConditions": [
            {
              "conditionPropertyName": "CaseOutcome",
              "conditionPropertyValue": "Other"
            }
          ]
        },
        "name": "OutcomeOther",
        "label": "Please provide further details"
      },
      {
        "component": "date",
        "name": "ResponseDate",
        "label": "Response date"
      }
    ],
    "IEDET_PSU_REGISTRATION": [
      {
        "component": "text",
        "name": "PsuReference",
        "label": "PSU reference"
      }
    ],
    "IEDET_PSU_TRIAGE": [
      {
        "props": {
          "choices": [
            {
              "label": "Yes",
              "value": "Accept"
            },
            {
              "label": "No - send back to IE Detention",
              "value": "ReturnCase"
            },
            {
              "label": "No - send to team not on DECS",
              "value": "CloseCase"
            }
          ]
        },
        "component": "radio",
        "name": "PsuTriageOutcome",
        "label": "Is this serious misconduct case for PSU to investigate?"
      },
      {
        "props": {
          "choices": [
            {
              "label": "Assault",
              "value": "Assault",
              "name": "CatAssault"
            },
            {
              "label": "Fraud / corruption",
              "value": "Fraud",
              "name": "CatFraud"
            },
            {
              "label": "Other unprofessionalism",
              "value": "OtherUnprof",
              "name": "CatOtherUnprof"
            },
            {
              "label": "Racism / discrimination",
              "value": "Racism",
              "name": "CatRacism"
            },
            {
              "label": "Rudeness",
              "value": "Rude",
              "name": "CatRude"
            },
            {
              "label": "Sexual assault",
              "value": "SexAssault",
              "name": "CatSexAssault"
            },
            {
              "label": "Theft",
              "value": "Theft",
              "name": "CatTheft"
            },
            {
              "label": "Unfair treatment",
              "value": "Unfair",
              "name": "CatUnfair"
            }
          ],
          "showLabel": false,
          "saveSeparately": true
        },
        "component": "checkbox-group",
        "name": "PsuCompCategoriesSerious",
        "label": "Serious misconduct"
      }
    ],
    "IEDET_PSU_OUTCOME": [
      {
        "props": {
          "choices": [
            {
              "label": "Substantiated",
              "value": "Substantiated"
            },
            {
              "label": "Partially substantiated",
              "value": "Partially substantiated"
            },
            {
              "label": "Unsubstantiated",
              "value": "Unsubstantiated"
            },
            {
              "label": "Withdrawn",
              "value": "Withdrawn"
            },
            {
              "label": "Not serious - send back to IE Detention",
              "value": "ReturnCase"
            }
          ]
        },
        "component": "radio",
        "name": "PsuComplaintOutcome",
        "label": "Complaint outcome"
      },
      {
        "props": {
          "visibilityConditions": [
            {
              "conditionPropertyName": "PsuComplaintOutcome",
              "conditionPropertyValue": "Withdrawn"
            }
          ]
        },
        "component": "text-area",
        "name": "WithdrawalReason",
        "label": "Why has the complaint been withdrawn?"
      },
      {
        "props": {
          "entity": "document",
          "choices": "CASE_DOCUMENT_LIST_IEDET_FINAL_RESPONSE",
          "hasAddLink": true,
          "hasRemoveLink": true
        },
        "component": "entity-list",
        "name": "FinalResponseDocument",
        "label": "Final response"
      },
      {
        "component": "date",
        "name": "DateOfResponse",
        "label": "Final response sent"
      }
    ]
  }
}
